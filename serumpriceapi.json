{
  "openapi": "3.0.0",
  "servers": [
    {
      "url": "https://price-api.sonar.watch"
    }
  ],
  "info": {
    "description": "Get mid price of a SPL token (based on a serum market)",
    "version": "0.0.1",
    "title": "Sonar - Serum Price API",
    "termsOfService": "",
    "contact": {
      "email": "sonar.watch.sol@gmail.com"
    },
    "x-logo": {
      "url": "https://sonar.watch/logos/sonar/sonar_logomark.png",
      "backgroundColor": "#FFFFFF",
      "altText": "Sonar logo"
    }
  },
  "tags": [
    {
      "name": "price",
      "description": "Operations about price"
    }
  ],
  "paths": {
    "/prices": {
      "get": {
        "tags": [
          "price"
        ],
        "summary": "Get all prices",
        "description": "",
        "parameters": [],
        "operationId": "",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ArrayOfPrices"
                }
              }
            }
          }
        }
      }
    },
    "/prices/{mint}": {
      "get": {
        "tags": [
          "price"
        ],
        "summary": "Find token price by mint address",
        "description": "",
        "operationId": "",
        "parameters": [
          {
            "name": "mint",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Price"
                }
              }
            }
          },
          "404": {
            "description": "NotFound",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        },
        "security": []
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Serum",
    "url": "https://github.com/project-serum/awesome-serum"
  },
  "components": {
    "schemas": {
      "Price": {
        "type": "object",
        "properties": {
          "mint": {
            "type": "string",
            "example": "So11111111111111111111111111111111111111112"
          },
          "symbol": {
            "type": "string",
            "example": "SOL"
          },
          "price": {
            "type": "number",
            "example": 12.22
          },
          "updatedAt": {
            "type": "string",
            "example": "2021-05-23T14:14:15.166Z"
          }
        }
      },
      "ArrayOfPrices": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Price"
        },
        "example": [
          {
            "mint": "11111111111111111111111111111111",
            "symbol": "SOL",
            "price": 22.8119,
            "updatedAt": "2021-05-23T14:14:14.286Z"
          },
          {
            "mint": "So11111111111111111111111111111111111111112",
            "symbol": "SOL",
            "price": 22.8119,
            "updatedAt": "2021-05-23T14:14:15.166Z"
          },
          {
            "mint": "kinXdEcpDQeHPEuQnqmUgtYykqKGVFq6CeVX5iAHJq6",
            "symbol": "KIN",
            "price": 0.000041,
            "updatedAt": "2021-05-23T14:14:15.983Z"
          },
          {
            "mint": "3K6rftdAaQYMPunrtNRHgnK2UAtjm2JwyT2oCiTDouYE",
            "symbol": "XCOPE",
            "price": 2.41,
            "updatedAt": "2021-05-23T14:14:16.867Z"
          }
        ]
      }
    },
    "securitySchemes": {},
    "links": {},
    "callbacks": {}
  },
  "security": []
}
